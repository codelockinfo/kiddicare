<div class="contactusbgimage" style ="background-image: url('https://cdn.shopify.com/s/files/1/0431/8039/2600/files/DSC00669_1_1.png?v=1655005746')">
  <h2 class="custm_contact_title">Contact us</h2>
</div>
<div class= "back_contact">
  <div class="contact_containes">

    <div class="custm_stars">
      <div class="custom_star1">
        {% section 'cusom_contact' %}
      </div>
      <div class="custom_star2">
        <div class="grid">
          <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
            <!--       <div class="section-header text-center">
<h1>{{ page.title }}</h1>
</div> -->

            {% if page.content.size > 0 %}
            <div class="rte">
              {{ page.content }}
            </div>
            {% endif %}

            <div class="clscontigue contact-form form-vertical">
              {%- assign formId = 'ContactForm' -%}
              {% form 'contact', id: formId %}
              {% include 'form-status', form: form, form_id: formId %}
              <div class="grid grid--half-gutters">
                <div class="clsform_table grid__item medium-up--one-half">
                  <label for="{{ formId }}-name">{{ 'contact.form.name' | t }}</label>
                  <input type="text" id="{{ formId }}-name" placeholder="Enter your name here*" name="contact[{{ 'contact.form.name' | t }}]" value="{% if form[name] %}{{ form[name] }}{% elsif customer %}{{ customer.name }}{% endif %}">
                </div>
                <div class="clsform_table  grid__item medium-up--one-half">
                  <label for="{{ formId }}-email">{{ 'contact.form.email' | t }} <span aria-hidden="true"></span></label>
                  <input
                         type="email"
                         id="{{ formId }}-email"
                         name="contact[email]"
                         autocorrect="off"
                         autocapitalize="off"
                         placeholder="Enter your email address here*"
                         value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
                         aria-required="true"
                         {%- if form.errors contains 'email' -%}
                         class="input--error"
                         aria-invalid="true"
                         aria-describedby="{{ formId }}-email-error"
                         {%- endif -%}
                         >
                  {%- if form.errors contains 'email' -%}
                  <span id="{{ formId}}-email-error" class="input-error-message">
                    <span class="visually-hidden">{{ 'general.accessibility.error' | t }} </span>
                    {% include 'icon-error' %}
                    <span>{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}.</span>
                  </span>
                  {%- endif -%}
                </div>
              </div>
              <div class="form_details">
                <label for="{{ formId }}-phone">{{ 'contact.form.phone' | t }}</label>
                <input type="tel" id="{{ formId }}-phone" name="contact[{{ 'contact.form.phone' | t }}]" placeholder="Enter your phone number here" pattern="[0-9\-]*" value="{% if form[phone] %}{{ form[phone] }}{% elsif customer %}{{ customer.phone }}{% endif %}">
              </div>	
              <div class="form_details">
                <label for="{{ formId }}-message">{{ 'contact.form.message' | t }}</label>
                <textarea rows="10" placeholder="Enter your message here*" id="{{ formId }}-message" name="contact[{{ 'contact.form.message' | t }}]">{% if form.body %}{{ form.body }}{% endif %}</textarea>
              </div>

              <input type="submit" class="btn clssubmitbtn" value="submit">
      
<div class="custom-model-main">
    <div class="custom-model-inner">        
    <div class="close-btn">Ã—</div>
        <div class="custom-model-wrap">
            <div class="pop-up-content-wrap">
               Content Here
            </div>
        </div>  
    </div>  
    <div class="bg-overlay"></div>
</div>               
              {% endform %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div>
    {% section 'googleMap' %}
  </div>
</div>


<style>
  .main-content {
    padding-top: 0px;
  }
  .contact_containes{
    margin: 0 10%;
  }
  .googlecount {
    padding-top: 6%;
    padding-bottom: 3%;
  }
  .maptitle {
    font-family: 'Hunterswood';
    font-style: normal;
    font-weight: 400;
    font-size: 80px;
    line-height: 65px;
    color: #84DADE;    
    margin-bottom: 0px;
    padding-bottom: 4%;
  }
  .contactusbgimage{
    height: 50vh;
    background-size: cover;
    background-repeat: no-repeat;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;

  }
  .contactusbgimage::before{
    content: " ";
    position: absolute;
    background: rgba(0, 0, 0, 0.2);
/*     height: 59vh; */
    top: -6px;
    bottom: 0%;
    right: 0%;
    left: 0%;
  }
  .custm_contact_title {
    text-align: center;
    color: #fff;
    font-family: 'Hunterswood';
    font-style: normal;
    font-weight: 400;
    font-size: 65px;
    line-height: 66px;
    letter-spacing: 0px;
    margin-bottom: 0px;
    position: relative;
  }
  .contactno {
    display: none;
  }
  .custm_stars{
    display: flex;
    flex-wrap: wrap;
  }
  .custom_star1{
    flex: 0 0 50%;
    max-width: 50%;
  }
  .custom_star2{
    flex: 0 0 50%;
    max-width: 50%;
  }

  .back_contact{
    background-color: #006781;
  }
  .clscontigue .medium-up--one-half {
    width: 100%;
  }

  .clscontigue  .grid__item.medium-up--one-half{
    display: flex;
    flex-wrap: wrap;
    align-items: baseline;
    justify-content: space-between;
  }
  .clscontigue label{
    width: 15%;
    display: inline;
    font-family: 'Hunterswood';
    font-style: normal;
    font-weight: 400;
    font-size: 30px;
    line-height: 45px;
    color: #84DADE;
    text-transform: uppercase;
  }
  .clscontigue.form-vertical input, .clscontigue.form-vertical textarea {
    width: 85%;
    display: inline;
    border-radius: 35px;
    padding: 18px 18px;
    font-size: 17px;
  }
  .form_details {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
  }
  .btn.clssubmitbtn {
    background-color: #F48F89;
    font-size: 20px;
    font-weight: 600;
    width: 38% !important;
    display: flex !important;
    margin-left: auto;
    justify-content: center;
    font-family: 'Hunterswood';
    font-style: normal;
    font-weight: 400;
    font-size: 30px !important;
    line-height: 36px;
    text-align: center;
    color: #FFFFFF;
    padding: 0px 0 17px;
  }
  .clscontigue input.btn.clssubmitbtn {
    padding: 0px 0 16px;
  }
  .mapalign iframe{
    border-radius: 40px;
  }
  .custom_googlemap {
    margin: 0 10%;
    margin-bottom: 10px;
  }

  @media only screen and (max-width: 1580px) {
    .contact_containes {
      padding: 0 0px 0px 60px;
    }
    .custom_star2 {
      flex: 0 0 60%;
      max-width: 60%;
    }
    .custom_star1 {
      flex: 0 0 40%;
      max-width: 40%;
    }
    .clscontigue.form-vertical input, .clscontigue.form-vertical textarea {
      width: 80%;
    }
    .clscontigue label {
      width: 19%;
    }

    @media only screen and (max-width: 1200px) {
      .custom_title{
        margin-bottom: 20px;
        font-size: 38px;
        line-height: 40px;
      }
      .general{
        font-size: 20px;
        line-height: 30px;
      }
      .contact_containes {
        padding: 0 0px 0px 20px;
      }
      .custom_star2 .medium-up--push-one-twelfth {
        left: 0%;
      }
      .custom_star2 .medium-up--five-sixths {
        width: 95.33333%;
      }
      .clscontigue label{
        font-size: 30px;
      }
      .custom_googlemap {
        padding: 0 0px;
      }
    }
    @media only screen and (max-width: 747px) {
      .contactusbgimage::before{
        top: -15px;
      }
    }

    @media only screen and (max-width: 768px) {
      .custom_star1 {
        flex: 0 0 100%;
        max-width: 100%;
      }
      .custom_star2 {
        flex: 0 0 100%;
        max-width: 100%;
      }
      .maptitle{
        text-align: center;
        font-size: 50px;
      }
      .custm_contact_title{          	
        font-size: 45px;
        line-height: 50px;
      }
    }
	
    @media only screen and (max-width: 749px) {
      .clscontigue.form-vertical input, .clscontigue.form-vertical textarea {
        width: 69%;
      }
    }
    
    @media only screen and (max-width: 414px) {
      .custom_title {
        margin-bottom: 20px;
        font-size: 35px;
        line-height: 35px;
      }
      .clscontigue label {
        font-size: 22px;
      }
      .clscontigue.form-vertical input, .clscontigue.form-vertical textarea{
        font-size: 14px;
      }
      .maptitle{
        font-size: 35px;
      }
    }

    @media only screen and (max-width: 360px) {
      .custm_contact_title {
        font-size: 35px;
        line-height: 40px;
      }
      .custom_title {
        margin-bottom: 12px;
        font-size: 33px;
        line-height: 35px;
      }
      .titlemail{
        font-size: 14px;
      }
      .clscontigue.form-vertical input, .clscontigue.form-vertical textarea {
        width: 78%;
      }
      .clscontigue label {
        font-size: 12px;
      }

    }
</style>
<script>    
$(".Click-here").on('click', function() {
  $(".custom-model-main").addClass('model-open');
}); 
$(".close-btn, .bg-overlay").click(function(){
  $(".custom-model-main").removeClass('model-open');
});               
</script>