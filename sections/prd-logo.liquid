    </div>
    <div class="All-logos">
      {% for block in section.blocks %}
      <div class="first-logo">
        <div class="clslogo-cls" data-src="{{block.settings.image-pop | img_url: 'master' }}"
             data-heading="{{block.settings.heading-pop}}" data-text="{{block.settings.text-pop}}">
          <img class="img-normal" src="{{block.settings.image | img_url: 'master'}}">
          <img class="img-hover" src="{{block.settings.hover | img_url: 'master'}}">
        </div>
