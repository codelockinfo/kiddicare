{% schema %}
{
"name": "Prd Logo",
"settings": [
{
"id" : "heading",
"type" : "text",
"label" : "Heading"
},
{
"id" : "text",
"type" : "textarea",
"label" : "Text"
},
{
"id" : "color",
"type" : "color",
"label" : "Background Color"
}
],
"blocks" : [
{
"name" : "block",
"type" : "block",
"settings" : [
{
"id" : "image",
"type" : "image_picker",
"label" : "Image"
},
{
"id" : "hover",
"type" : "image_picker",
"label" : "Hover Image"
},
{
"type": "header",
"content": "Popup Details"
},
{
"id" : "image-pop",
"type" : "image_picker",
"label" : "Popup Image"
},
{
"id" : "heading-pop",
"type" : "text",
"label" : "Heading"
},
{
"id" : "text-pop",
"type" : "textarea",
"label" : "Text"
}
]
}
],
"presets" : [
{
"name" : "Prd Logo"
}
]
}
{% endschema %}


<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>
  $(".logo-cls").on('click', function () {
    $(this).closest(".first-logo").find(".custom-model-main").addClass('model-open');
  });
  $(".close-btn, .bg-overlay").click(function () {
    $(".custom-model-main").removeClass('model-open');
  });

</script>