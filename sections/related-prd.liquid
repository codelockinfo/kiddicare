<div class="custom_container animated fadeInUp">
  <h3 class="health_title text-cen">{{section.settings.titlep}}</h3>
  <div class="avenuue">
    <div class="them_nava multiple-items">
      {% for block in section.blocks %}
      {%- assign collection = collections[block.settings.collection] -%}


      {%- for product in collection.products %}
      <div class="collect_flx25 ">
        <div class="bx_shdow">
          <div class="hav_pro">
            {% if product.featured_image != blank %}
            <a href="{{ product.url}}" class="cool_img"> <img
                data-src="{{ product.featured_image | img_url: 'master' }}" / class="lazyload avenue_img"></a>
            {% endif %}
          </div>
          <div class="prtilte">
            <p class="custom_call"> <a href="{{product.url}}">{{ product.title }}</a></p>
          </div>
          <div class="text-cent">
            <div class="starrank">
              {% if product.metafields.my_fields.star == 1 %}
              <span class="fa fa-star checked"></span>
              {% elsif product.metafields.my_fields.star == 2 %}
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              {% elsif product.metafields.my_fields.star == 3 %}
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              {% elsif product.metafields.my_fields.star == 4 %}
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              {% elsif product.metafields.my_fields.star == 5 %}
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star checked"></span>
              {% endif %}
              {% if product.metafields.my_fields.review_count != blank %}
              <span class="count">({{ product.metafields.my_fields.review_count }} )</span>
              {% endif %}
            </div>

            <span class="kg-title">{{ product.metafields.my_fields.prd_kg}}</span>

          </div>

          <div class="posi-abso">
            <h2>{{ product.price | money_without_trailing_zeros }}</h2>
          </div>
        </div>
      </div>
      {% endfor %}

      {% endfor %}

    </div>
  </div>
</div>


