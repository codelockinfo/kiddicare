  "label" : "Bbutton"
  },
  {
  "id" : "btn-link",
  "type" : "url",
  "label" : "Button Text"
  }
  ],
  "presets" : [
  {
  "name" : "Free Sample"
  }
  ]
  }
  {% endschema %}


  <script>
    $(".smp-btn").on('click', function() {
      $(this).closest(".Free-Sample-section").find(".custom-model-main").addClass('model-open');
    }); 
    $(".close-btn, .bg-overlay").click(function(){
      $(".custom-model-main").removeClass('model-open');
    });

  </script>

